<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/global_gray"
    android:orientation="vertical" >

    <RelativeLayout style="@style/title_container" >

        <TextView
            android:id="@+id/get_back"
            style="@style/title_text_back" />

        <TextView
            style="@style/title_text_body"
            android:text="总收益" />
    </RelativeLayout>

	<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp" 
        android:background="@android:color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:layout_marginLeft="20dp"
            android:text="总收益"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_zong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginRight="20dp"
            android:text="0.00元"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/global_yellowcolor" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingLeft="20dp"
        android:background="@color/global_gray"
        android:gravity="center_vertical"
        android:text="包含："
        android:textColor="@color/text_gray"
        android:textSize="14sp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_width" />

    <RelativeLayout 
        android:id="@+id/rl_fanheleiji"
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:background="@android:color/white">

        <TextView
            style="@style/contact_item"
            android:drawableLeft="@drawable/fanhezichan"
            android:text="饭盒累计收益"
            android:textSize="14sp" 
            android:gravity="center_vertical"/>

        <TextView
            android:id="@+id/tv_fanhe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/arrow"
            android:drawablePadding="10dp"
            android:paddingRight="12dp"
            android:text="0.00元"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />
    </RelativeLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_width" />

    <RelativeLayout 
        android:id="@+id/rl_fanwanleiji"
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:background="@android:color/white">

        <TextView
            style="@style/contact_item"
            android:drawableLeft="@drawable/fanwanzichan"
            android:text="饭碗累计收益"
            android:textSize="14sp" 
            android:gravity="center_vertical"/>

        <TextView
            android:id="@+id/tv_fanwan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/arrow"
            android:drawablePadding="10dp"
            android:paddingRight="12dp"
            android:text="0.00元"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />
    </RelativeLayout>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_width" />
    <RelativeLayout 
        android:id="@+id/rl_fantongleiji"
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:background="@android:color/white">

        <TextView
            style="@style/contact_item"
            android:drawableLeft="@drawable/fantongzichan"
            android:text="饭桶累计收益"
            android:textSize="14sp" 
            android:gravity="center_vertical"/>

        <TextView
            android:id="@+id/tv_fantong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/arrow"
            android:drawablePadding="10dp"
            android:paddingRight="12dp"
            android:text="0.00元"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />
    </RelativeLayout>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_width" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/line_width" />

   <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:background="@android:color/white">

        <TextView
            style="@style/contact_item"
            android:drawableLeft="@drawable/fanwandaishoushouyi"
            android:text="饭碗待收收益"
            android:textSize="14sp" 
            android:gravity="center_vertical"/>

        <TextView
            android:id="@+id/tv_daishou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="30dp"
            android:text="0.00元"
            android:textColor="@color/global_yellowcolor"
            android:textSize="14sp" />
    </RelativeLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_width" />
    <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="15dp"
                    android:drawableLeft="@drawable/little"
                    android:gravity="center"
                    android:drawablePadding="3dp"
                    android:text="小贴士：饭盒累计收益、饭碗累计收益、饭桶累计收益，均指已到账的收益。"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />
</LinearLayout>